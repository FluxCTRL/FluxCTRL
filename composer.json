{
    "name": "fluxctrl/fluxctrl",
    "description": "Minimalistic information flux management tool.",
    "homepage": "http://fluxctrl.io",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "mobiledetect/mobiledetectlib": "2.*",
        "cakephp/migrations": "dev-master",
        "cakephp/plugin-installer": "*",
        "friendsofcake/crud": "dev-cake3",
        "fguillot/picofeed": "~0.1.2",
        "gourmet/knp-menu": "dev-master"
    },
    "require-dev": {
        "d11wtq/boris": "1.0.*",
        "cakephp/debug_kit": "~3.0",
        "cakephp/bake": "~1.0",
        "cakephp/codeception": "dev-master",
        "gourmet/muffin": "dev-master",
        "flow/jsonpath": "~0.2.4"
    },
    "autoload": {
        "psr-4": {
            "FluxCtrl\\App\\": "src",
            "FluxCtrl\\Clear\\": "plugins/FluxCtrl/Clear/src",
            "FluxCtrl\\Kbd\\": "plugins/FluxCtrl/Kbd/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FluxCtrl\\App\\Test\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": "FluxCtrl\\App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
